{
  "title": "Inference API - Dynamic Batcher",
  "uid": "inference-batcher",
  "version": 1,
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Request Rate (req/s)",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "rate(batcher_requests_total[1m])",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "reqps", "label": "Requests/sec"},
        {"format": "short"}
      ]
    },
    {
      "id": 2,
      "title": "Error Rate (%)",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "100 * rate(batcher_requests_total{status=\"error\"}[1m]) / rate(batcher_requests_total[1m])",
          "legendFormat": "error %",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "percent", "label": "Error Rate"},
        {"format": "short"}
      ]
    },
    {
      "id": 3,
      "title": "Request Latency (p50, p95, p99)",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(batcher_request_latency_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(batcher_request_latency_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(batcher_request_latency_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "yaxes": [
        {"format": "s", "label": "Latency"},
        {"format": "short"}
      ]
    },
    {
      "id": 4,
      "title": "Average Batch Size",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "rate(batcher_batch_size_sum[1m]) / rate(batcher_batch_size_count[1m])",
          "legendFormat": "avg batch size",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "short", "label": "Batch Size"},
        {"format": "short"}
      ]
    },
    {
      "id": 5,
      "title": "Queue Size",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "batcher_queue_size",
          "legendFormat": "queue size",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "short", "label": "Requests"},
        {"format": "short"}
      ]
    },
    {
      "id": 6,
      "title": "Inflight Batches",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "batcher_inflight_batches",
          "legendFormat": "inflight",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "short", "label": "Batches"},
        {"format": "short"}
      ]
    },
    {
      "id": 7,
      "title": "Batch Wait Time (p50, p95, p99)",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(batcher_batch_wait_time_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(batcher_batch_wait_time_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(batcher_batch_wait_time_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "yaxes": [
        {"format": "s", "label": "Wait Time"},
        {"format": "short"}
      ]
    },
    {
      "id": 8,
      "title": "Inference Time (p50, p95, p99)",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(batcher_inference_time_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(batcher_inference_time_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(batcher_inference_time_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "yaxes": [
        {"format": "s", "label": "Inference Time"},
        {"format": "short"}
      ]
    },
    {
      "id": 9,
      "title": "Batch Size Distribution",
      "type": "graph",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
      "targets": [
        {
          "expr": "rate(batcher_batch_size_bucket[5m])",
          "legendFormat": "â‰¤{{le}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "short", "label": "Rate"},
        {"format": "short"}
      ]
    }
  ]
}
